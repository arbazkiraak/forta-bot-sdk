{
  "name": "forta-bot-starter",
  "version": "0.0.1",
  "description": "Forta Bot Typescript starter project",
  "chainIds": [
    1
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,ts,json  --exec \"npm run build && forta-bot run\"",
    "start:prod": "forta-bot run --prod",
    "tx": "npm run build && forta-bot run --tx",
    "block": "npm run build && forta-bot run --block",
    "range": "npm run build && forta-bot run --range",
    "file": "npm run build && forta-bot run --file",
    "publish": "forta-bot publish",
    "info": "forta-bot info",
    "logs": "forta-bot logs",
    "push": "forta-bot push",
    "disable": "forta-bot disable",
    "enable": "forta-bot enable",
    "keyfile": "forta-bot keyfile",
    "test": "jest"
  },
  "dependencies": {
    "forta-agent": "^0.1.9"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/nodemon": "^1.19.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.8",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  }
}
